    "gcloud": "^0.24.0",
    "googleapis": "^2.1.5",
    "jade": "^1.11.0",
    "kerberos": "^0.0.17",
    "lodash": "^3.10.1",
    "mongodb": "^2.0.46",
    "multer": "^1.1.0",
    "mysql": "^2.9.0",
    "prompt": "^0.2.14",
    "request": "^2.65.0",
    "winston": "^1.1.1"
  },
  "devDependencies": {
    "coveralls": "^2.11.4",
    "istanbul": "^0.4.1",
    "jshint": "^2.8.0",
    "mocha": "^2.3.4",
    "proxyquire": "^1.7.3",
    "sinon": "^1.17.2",
    "supertest": "^1.1.0"
  },
  "engines": {
